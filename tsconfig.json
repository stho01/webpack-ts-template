{
    "compilerOptions": {
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2015.core",
            "es2015.symbol",
            "es2015.promise",
            "es2015.iterable",
            "es2015.collection"
        ],
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules"
    ]
}
